{
    "version": 2,
    "name": "radddio",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/socket.io/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/f/(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1",
            "check": true
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}